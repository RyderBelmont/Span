 <?xml version="1.0" encoding="UTF-8"?>

<config>
    <debug>false</debug> <!-- true or false -->
    <collectibles>true</collectibles>

    <!-- Default / startup table fill mode. -->
    <default-fill>new</default-fill>

    <!-- Options for alt costume availability. -->
    <alternate-costumes>true</alternate-costumes>
    <alternate-costume-sets>all</alternate-costume-sets>
    
    <!-- Options related to events. -->
    <!--<force-alternate-costume>halloween</force-alternate-costume>-->
    <default-background>inventory</default-background>
   
    <!-- Set to true while a resort is running. -->
    <resort>false</resort>

    <!-- Metadata filled in by CI. -->
    <commit>__CI_COMMIT_SHA</commit>
    <version-tag>__VERSION</version-tag>
</config>
